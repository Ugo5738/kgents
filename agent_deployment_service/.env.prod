# Production environment configuration for agent_deployment_service
# Service Configuration
AGENT_DEPLOYMENT_SERVICE_ENVIRONMENT=production
AGENT_DEPLOYMENT_SERVICE_LOGGING_LEVEL=INFO
AGENT_DEPLOYMENT_SERVICE_ROOT_PATH=/api/v1

# Database connection
AGENT_DEPLOYMENT_SERVICE_DATABASE_URL=postgresql+psycopg://postgres:postgres@supabase_db_kgents:5432/agent_deployment_prod_db?options=-c%20search_path%3Dpublic
AGENT_DEPLOYMENT_SERVICE_POSTGRES_USER=postgres
AGENT_DEPLOYMENT_SERVICE_POSTGRES_PASSWORD=postgres
AGENT_DEPLOYMENT_SERVICE_POSTGRES_DB=agent_deployment
AGENT_DEPLOYMENT_SERVICE_POSTGRES_HOST=postgres
AGENT_DEPLOYMENT_SERVICE_POSTGRES_PORT=5432

# Services configuration
AUTH_SERVICE_URL=http://auth_service:8000
LANGFLOW_API_URL=http://langflow:7860

# API configuration
TOKEN_URL=${AUTH_SERVICE_URL}/api/v1/auth/validate-token
CORS_ORIGINS=["https://api.kgents.com", "https://admin.kgents.com", "https://app.kgents.com"]