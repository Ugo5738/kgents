# Development environment configuration for agent_management_service
# Service Configuration
AGENT_MANAGEMENT_SERVICE_ENVIRONMENT=development
AGENT_MANAGEMENT_SERVICE_LOGGING_LEVEL=INFO
AGENT_MANAGEMENT_SERVICE_ROOT_PATH=/api/v1

# Database connection
AGENT_MANAGEMENT_SERVICE_DATABASE_URL=postgresql+psycopg://postgres:postgres@127.0.0.1:54322/agent_management_dev_db
AGENT_MANAGEMENT_SERVICE_POSTGRES_USER=postgres
AGENT_MANAGEMENT_SERVICE_POSTGRES_PASSWORD=postgres
