AGENT_MANAGEMENT_SERVICE_POSTGRES_HOST=127.0.0.1
AGENT_MANAGEMENT_SERVICE_POSTGRES_PORT=54322

# Services configuration
AGENT_MANAGEMENT_SERVICE_AUTH_SERVICE_URL=http://auth_service:8000
