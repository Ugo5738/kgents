# Service Configuration
AGENT_MANAGEMENT_SERVICE_ENVIRONMENT=development
AGENT_MANAGEMENT_SERVICE_LOGGING_LEVEL=INFO
AGENT_MANAGEMENT_SERVICE_ROOT_PATH=/api/v1

# Database connection
AGENT_MANAGEMENT_SERVICE_DATABASE_URL=postgresql+asyncpg://postgres:postgres@postgres:5432/agent_management
AGENT_MANAGEMENT_SERVICE_POSTGRES_USER=postgres
AGENT_MANAGEMENT_SERVICE_POSTGRES_PASSWORD=postgres
AGENT_MANAGEMENT_SERVICE_POSTGRES_DB=agent_management
AGENT_MANAGEMENT_SERVICE_POSTGRES_HOST=postgres
AGENT_MANAGEMENT_SERVICE_POSTGRES_PORT=5432

# Services configuration
AUTH_SERVICE_URL=http://auth_service:8000
LANGFLOW_API_URL=http://langflow:7860

# API configuration
TOKEN_URL=${AUTH_SERVICE_URL}/api/v1/auth/validate-token
CORS_ORIGINS=["*"]

